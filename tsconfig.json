{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@data/*": ["./src/data/*"],
      "@images/*": ["./src/images/*"],
      "@layout/*": ["./src/layout/*"],
      "@pages/*": ["./src/pages/*"],
      "@styles/*": ["./src/styles/*"],
      "@types": ["./src/types"],
      "@utils/*": ["./src/utils/*"],
    }
  }
}
